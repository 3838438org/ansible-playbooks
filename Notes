#Increase disk space:
dd if=/dev/zero of=/tmp/tempFiller.deleteMe bs=1M count=300000

#Fork bomb to test load
:(){ :|: & };:

#To check if spdy is running execute this on the server
openssl s_client -connect localhost:443 -nextprotoneg ''

#Create SSH key:
ssh-keygen -t rsa

#Copy ssh keys
mkdir .ssh && echo "ssh-rsa  nkadithya31@gmail.com" >> .ssh/authorized_keys

#Passowrdless sudo
sudo su && sudo echo "$USER ALL=(ALL) NOPASSWD:ALL" | sudo tee -a /etc/sudoers && logout

#Validate ssl certificate
openssl s_client -showcerts -connect localhost:443
openssl s_client -CApath /etc/nginx/ssl -connect localhost:443

