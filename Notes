#Glassfish url
http://download.java.net/glassfish/4.1/release/glassfish-4.1.zip
#Set glassfish admin password
sudo /opt/glassfish4/bin/asadmin  --port 4848 change-admin-password
#Enable glassfish admin
sudo /opt/glassfish4/bin/asadmin  --port 4848 enable-secure-admin
